{
  "version": "0.2.0",
  "configurations": [
  {
    "name": ".NET Core Launch (console)",
    "type": "coreclr",
    "request": "launch",
    "preLaunchTask": "build",
    "program": "${workspaceFolder}/BlogData/bin/Debug/netstandard2.1/BlogData.dll",
    "args": [],
    "cwd": "${workspaceFolder}/BlogData",
    "stopAtEntry": false,
    "console": "internalConsole"
  },
    {
      "name": "Attach to .NET Functions",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:azureFunctions.pickProcess}"
    }
  ]
}
